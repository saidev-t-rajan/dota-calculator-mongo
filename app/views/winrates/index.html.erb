<h1>Winrates</h1>
<table class='table table-striped table-hover'>
  <thead>
    <tr>
      <th>Type</th>
      <th>Time</th>
      <th>Skill</th>
      <th>Last Updated</th>
      <th>Example URL</th>
      <th>Combo</th>
      <th>Anti</th>
    </tr>
  </thead>
  <tbody>
    <% @winrates.each do |winrate| %>
      <tr>
        <td><%= winrate.type %></td>
        <td><%= winrate.time %></td>
        <td><%= winrate.skill %></td>
        <td><%= winrate.last_updated %></td>
        <td><%= link_to truncate(winrate.heros.first.url_combo, length: 50), winrate.heros.first.url_combo %></td>
        <td><span class="glyphicon glyphicon-download" aria-hidden="true"></span></td>
        <td><span class="glyphicon glyphicon-download" aria-hidden="true"></span></td>
        <td><%= link_to 'Destroy', winrate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Winrate', new_winrate_path %>